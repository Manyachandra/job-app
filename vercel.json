{
  "version": 2,
  "buildCommand": "npm run build:all",
  "outputDirectory": "frontend/build",
  "functions": {
    "backend/src/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
